{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "language": "ts",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "properties": {
    "aws": {
      "type": "object",
      "properties": {
        "accessKeyId": {
          "type": "string"
        },
        "secretAccessKey": {
          "type": "string"
        },
        "region": {
          "type": "string"
        }
      },
      "required": [
        "accessKeyId",
        "secretAccessKey",
        "region"
      ]
    }
  }
}
